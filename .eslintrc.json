{
  "extends": [
    "next/core-web-vitals",
    "@rocketseat/eslint-config/react"
  ],
  "rules": {
    "react/no-unknown-property": [
      "error",
      {
        "ignore": [
          "clipPath",
          "stroke-linecap",
          "stroke-width",
          "stroke-linejoin"
        ]
      }
    ],
    "@next/next/no-img-element": "disabled"
  }
}
