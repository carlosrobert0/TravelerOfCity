{
  "extends": [
    "next/core-web-vitals",
    "@rocketseat/eslint-config/react"
  ],
  "rules": {
    "react/no-unknown-property": [
      "error",
      {
        "ignore": [
          "clipPath",
          "stroke-linecap",
          "stroke-width",
          "stroke-linejoin"
        ]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "parser": "flow"
      }
    ],
    "camelcase": "off",
    "@next/next/no-img-element": "off"
  }
}
